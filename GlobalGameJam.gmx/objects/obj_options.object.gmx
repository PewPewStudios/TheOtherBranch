            }
            if(selected = 2)
            {
                if(parayea = "ON")
                {
                    parayea = "OFF";
                    global.parachute = false;
                }
                else if(parayea = "OFF")
                {
                    parayea = "ON";
                    global.parachute = true;
                }
            }
            goodToPress = false;
            alarm[0] = 7.5;
        }
        if(selected = 0)
        {
            if(gamepad_button_check_pressed(0,gp_stickl) &amp;&amp; goodToPress )
            {
                parachuteMode++;
                goodToPress = false;
                alarm[0] = 7.5;
            }
        }
        if(gamepad_axis_value(0, gp_axislv) &gt; .4)
        {
            selected++;
            goodToPress = false;
            alarm[0] = 7.5;
        }
        if(gamepad_axis_value(0, gp_axislv) &lt; -.4)
        {
            selected--;
            goodToPress = false;
            alarm[0] = 7.5;
        }
        if(gamepad_button_check_pressed(0,gp_face2))
        {
            room=rm_main_menu;
        }
    }
}
